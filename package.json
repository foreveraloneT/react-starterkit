{
  "name": "dashboard-starterkit",
  "version": "1.0.0",
  "description": "Starter template react for web team using webpack4",
  "main": "server.js",
  "scripts": {
    "start": "npm run clean:dev && webpack-dev-server --mode development --config webpack.config.dev.js --open",
    "deploy": "npm run clean && webpack --mode production --config webpack.config.prod.js",
    "deploy:dev": "npm run clean:dev && webpack --mode development --config webpack.config.dev.js",
    "clean": "rimraf public/*",
    "clean:dev": "rimraf dev/*",
    "server": "node server.js",
    "storybook": "start-storybook -p 6010",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:only": "jest",
    "test:junit": "jest --coverage --ci --testResultsProcessor=\"jest-junit\""
  },
  "author": "Scale360: Sutee S.",
  "license": "ISC",
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.10",
    "@storybook/addon-backgrounds": "^3.0.0",
    "@storybook/addon-info": "^3.2.10",
    "@storybook/addon-links": "^3.2.10",
    "@storybook/addon-notes": "^3.2.10",
    "@storybook/addon-options": "^3.2.10",
    "@storybook/react": "^3.2.10",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^3.18.0",
    "eslint-config-airbnb-base": "^11.1.1",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "jest": "^22.0.5",
    "jest-junit": "^3.6.0",
    "mini-css-extract-plugin": "^0.2.0",
    "mock-local-storage": "^1.0.5",
    "node-sass": "^4.8.3",
    "react-hot-loader": "^4.0.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.5",
    "connect-history-api-fallback": "^1.5.0",
    "event-source-polyfill": "^0.0.12",
    "express": "^4.16.3",
    "html-webpack-plugin": "^3.0.7",
    "lodash": "^4.17.5",
    "moment": "^2.21.0",
    "moment-timezone": "^0.5.14",
    "numeral": "^2.0.6",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-highlight": "^0.12.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-form": "^7.3.0",
    "redux-logger": "^3.0.6",
    "redux-observable": "^0.18.0",
    "reselect": "^3.0.1",
    "rxjs": "^5.5.7",
    "styled-components": "^3.2.3"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|svg|gif)$": "<rootDir>/jest/fileMock.js",
      "^.+\\.(css|scss)$": "<rootDir>/jest/styleMock.js"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "(.mock)\\.(ts|tsx|js)$",
      "/distribution/.*\\.(ts|js)$"
    ],
    "setupTestFrameworkScriptFile": "mock-local-storage",
    "setupFiles": [
      "<rootDir>/jest/global-config-mock.js"
    ]
  }
}
